{
  "name": "New Tab Cat",
  "version": "1.2",
  "description": "A cat, growing at every tab open!",
  "incognito": "split",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2,
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
